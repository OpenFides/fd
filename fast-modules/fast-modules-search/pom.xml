<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <artifactId>fast-modules-search</artifactId>
    <description>fast data project for Spring Boot</description>
    <packaging>jar</packaging>
    <parent>
        <groupId>cn.zhumingwu</groupId>
        <artifactId>fast-modules</artifactId>
        <version>1.1.0</version>
        <relativePath>../pom.xml</relativePath>
    </parent>
    <properties>
        <es.version>7.3.2</es.version>
        <jackjson.version>2.10.2</jackjson.version>
    </properties>
    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>fast-database</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>logger-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>monitor-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>org.elasticsearch.client</groupId>
            <artifactId>elasticsearch-rest-high-level-client</artifactId>
            <version>${es.version}</version>
        </dependency>
    </dependencies>
    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>
</project>