<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <artifactId>fast-data-modules-client</artifactId>
    <description>fast data client</description>
    <packaging>jar</packaging>
    <parent>
        <groupId>ltd.fdsa</groupId>
        <artifactId>fast-data-modules</artifactId>
        <version>1.0.4</version>
        <relativePath>../pom.xml</relativePath>
    </parent>
    <properties/>
    <dependencies>
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>fast-data-common</artifactId>
        </dependency>
        <!-- 必选，用于快速创建数据库访问 -->
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>fast-data-database</artifactId>
        </dependency>
        <!-- 可选，自动记录日志到日志中心 -->
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>logger-spring-boot-starter</artifactId>
        </dependency>
        <!-- 可选，自动生成数据库的增删改查 -->
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>jdbc-spring-boot-starter</artifactId>
        </dependency>
        <!-- 可选，自动加入到监控中心 -->
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>monitor-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>remote-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>remote-consul-spring-boot-starter</artifactId>
        </dependency>
        <!-- 根据实现情况选择数据库 -->
        <dependency>
            <groupId>org.postgresql</groupId>
            <artifactId>postgresql</artifactId>
        </dependency>
        <dependency>
            <groupId>mysql</groupId>
            <artifactId>mysql-connector-java</artifactId>
        </dependency>
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>fast-data-database-jpa</artifactId>
        </dependency>
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>fast-data-database-mybatis</artifactId>
        </dependency>
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>fast-data-database-mybatis-generic</artifactId>
        </dependency>
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>fast-data-database-mybatis-plus</artifactId>
        </dependency>
        <!-- spring -->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>fast-data-modules-api</artifactId>
            <version>1.0.4</version>
        </dependency>
        <dependency>
            <groupId>ltd.fdsa</groupId>
            <artifactId>fast-data-core-web</artifactId>
            <version>1.0.4</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-devtools</artifactId>
            <optional>true</optional>
        </dependency>
    </dependencies>
</project>