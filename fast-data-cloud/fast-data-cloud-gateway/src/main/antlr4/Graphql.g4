grammar Graphql;import GraphqlCommon;@header{package ltd.fdsa.antlr.fql;}@lexer::members {    public boolean isDigit(int c) {        return c >= '0' && c <= '9';    }    public boolean isNameStart(int c) {        return '_' == c ||          (c >= 'A' && c <= 'Z') ||          (c >= 'a' && c <= 'z');    }    public boolean isDot(int c) {        return '.' == c;    }}document : selectionSet;selectionSet :  '{' selection+ '}';selection :alias? name arguments? directives? selectionSet?;alias : name ':';