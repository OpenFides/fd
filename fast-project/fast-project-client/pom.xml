<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <artifactId>fast-project-client</artifactId>
    <description>fast data client</description>
    <packaging>jar</packaging>
    <parent>
        <groupId>cn.zhumingwu</groupId>
        <artifactId>fast-project</artifactId>
        <version>1.1.0</version>
        <relativePath>../pom.xml</relativePath>
    </parent>
    <properties/>
    <dependencies>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>fast-core-base</artifactId>
            <version>1.1.0</version>
        </dependency>
        <!-- 必选，用于快速创建数据库访问 -->
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>fast-database-base</artifactId>
            <version>1.1.0</version>
        </dependency>
        <!-- 可选，自动记录日志到日志中心 -->
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>logger-spring-boot-starter</artifactId>
            <version>1.1.0</version>
        </dependency>
        <!-- 可选，自动生成数据库的增删改查 -->
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>jdbc-spring-boot-starter</artifactId>
        </dependency>
        <!-- 可选，自动加入到监控中心 -->
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>monitor-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>remote-spring-boot-starter</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>register-spring-boot-starter</artifactId>
        </dependency>
        <!-- 根据实现情况选择数据库 -->
        <dependency>
            <groupId>org.postgresql</groupId>
            <artifactId>postgresql</artifactId>
        </dependency>
        <dependency>
            <groupId>mysql</groupId>
            <artifactId>mysql-connector-java</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>fast-database-jpa</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>fast-database-mybatis</artifactId>
            <version>1.1.0</version>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>fast-database-mybatis-generic</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>fast-database-mybatis-plus</artifactId>
            <version>1.1.0</version>
        </dependency>
        <!-- spring -->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>fast-project-api</artifactId>
            <version>1.1.0</version>
        </dependency>
        <dependency>
            <groupId>cn.zhumingwu</groupId>
            <artifactId>fast-core-web</artifactId>
            <version>1.1.0</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-devtools</artifactId>
            <optional>true</optional>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>
</project>